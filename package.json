{
  "name": "web-summarizer-js",
  "version": "1.0.0",
  "description": "A Node.js web summarizer that extracts and summarizes website content using OpenAI API",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node main.js",
    "dev": "node main.js",
    "cli": "node cli.js",
    "summary": "node cli.js summary",
    "summary:openai": "node cli.js summary --model openai",
    "summary:llama": "node cli.js summary --model llama",
    "links": "node cli.js links",
    "links:openai": "node cli.js links --model openai",
    "links:llama": "node cli.js links --model llama",
    "interactive": "node cli.js interactive"
  },
  "bin": {
    "web-summarizer": "./cli.js"
  },
  "keywords": [
    "web-scraping",
    "summarizer",
    "openai",
    "nodejs",
    "javascript"
  ],
  "author": "keljohe10",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^9.4.1",
    "dotenv": "^16.0.0",
    "jsdom": "^22.1.0",
    "openai": "^3.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
